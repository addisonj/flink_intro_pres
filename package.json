{
  "name": "lambda_kappa_pres",
  "version": "1.0.0",
  "description": "A presentation",
  "main": "index.js",
  "dependencies": {
    "node-sass": "^4.3.0",
    "reveal-md": "2.3.1"
  },
  "scripts": {
    "presentg": "reveal-md -h 0.0.0.0 --theme ./theme/custom.css pres.md",
    "present": "reveal-md --theme ./theme/custom.css pres.md",
    "print": "reveal-md --theme ./theme/custom.css --print pres.pdf pres.md",
    "build": "node-sass theme/custom.scss theme/custom.css",
    "run": "npm run build && npm run present"
  },
  "devDependencies": {},
  "author": "",
  "license": "ISC"
}
